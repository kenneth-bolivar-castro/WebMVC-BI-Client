@model IEnumerable<WebMVC_BI_Client.Models.ApplicationUser>
@*using WebMVC_BI_Client.Models*@
@{
    ViewBag.Title = "UsersList";
}

<h2>UsersList</h2>

<p>
    @Html.ActionLink("Registrar", "Register")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
       <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApiToken)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApiToken)
            </td>

            <td>
             @*   @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |*@
              @*  @Html.ActionLink("Detalles", "Details", new { id = item.Id }) | *@
                @Html.ActionLink("Eliminar", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
