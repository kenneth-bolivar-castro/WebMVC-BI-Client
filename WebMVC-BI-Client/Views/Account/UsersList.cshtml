@model IEnumerable<WebMVC_BI_Client.Models.ApplicationUser>
@*@model IEnumerable<WebMVC_BI_Client.Models.> *@
@*using WebMVC_BI_Client.Models*@
@{
    ViewBag.Title = "UsersList";
}

<h2>UsersList</h2>

<p>
    @Html.ActionLink("Registrar", "RegisterUD")
</p>
<table class="table">
    <tr>
        <th>
            Id
        </th>
        <th>
            Nombre de Usuario
        </th>
        <th>
            Api Token
        </th>
        <th>
            Role
        </th>
        <th>

        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ApiToken)
        </td>
        <td>
            @ViewBag.UsRoNa[item.Id]
        </td>
        
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.Id }) |
            @*  @Html.ActionLink("Detalles", "Details", new { id = item.Id }) | *@
            @Html.ActionLink("Eliminar", "Delete", new { id = item.Id })
        </td>
    </tr>
    }

</table>
